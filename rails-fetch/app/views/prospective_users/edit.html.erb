<h1>Editing prospective_user</h1>

<% form_for(@prospective_user) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :website %><br />
    <%= f.text_field :website %>
  </p>
  <p>
    <%= f.label :heard_from %><br />
    <%= f.text_field :heard_from %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @prospective_user %> |
<%= link_to 'Back', prospective_users_path %>
